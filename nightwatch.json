{
  "page_objects_path": "./mapping/",
  "src_folders": ["test"],
  "output_folder": "reports",
  "selenium": {
    "start_process": true,
    "server_path": "./node_modules/selenium-server/lib/runner/selenium-server-standalone-3.141.59.jar",
    "log_path": "",
    "port": 4446,
    "cli_args": {
      "webdriver.chrome.driver": "./node_modules/chromedriver/lib/chromedriver/chromedriver"
    }
  },
  "test_settings": {
    "end_session_on_fail": false,
    "skip_testcases_on_fail": false,
    "default": {
      "selenium_port": 9530,
      "selenium_host": "localhost",
      "silent": true,
      "screenshots": {
        "enabled": true,
        "path": "./screenshots",
        "on_failure": true,
        "on_error": true
      },
      "desiredCapabilities": {
        "nativeEvents": false,
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "chromeOptions": {
          "args": ["sandbox", "disable-gpu", "start-maximized"],
          "w3c": false
        }
      }
    }
  }
}
